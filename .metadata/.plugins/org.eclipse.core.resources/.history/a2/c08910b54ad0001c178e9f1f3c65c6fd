package training;

public class Box {
	int width;
	int height;
	int depth;
	public void setwidth(int a) {
		this.width=a;
	}
	public void setheight(int b) {
		this.height=b;
	}
	public void setdepth(int c) {
		this.depth=c;
	}
	public void box(int a,int b,int c) {
		this.width=a;
		this.height=b;
		this.width=c;
	}
 public int volume (int a,int b, int c) {
	 int volume;
	 volume=(a*b*c);
	 System.out.print(volume);
	 return volume;
 }
}
