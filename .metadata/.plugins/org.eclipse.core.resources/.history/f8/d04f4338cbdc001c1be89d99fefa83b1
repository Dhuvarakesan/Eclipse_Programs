package logic;

public class Primebit {
	public static int  primebet(int num) {
		boolean f= true;
		for(int i=2;i<num;++i) {
				if(num%i==0) {
					f=false;
					break;
				}
			}
			if(f)
				System.out.print(num+" "+"prime");
			return num;
			

	}
	public static void main(String[] args) {
		/*
		 * int num=15; Primebit a=new Primebit(); int result= a.primebet(num);
		 */
	System.out.println(primebet(7));
	}

}
