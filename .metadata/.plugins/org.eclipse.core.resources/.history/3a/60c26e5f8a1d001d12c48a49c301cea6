package logic;

public class Zoho1 {

	public static void main(String[] args) {
		String in="dhuvarakesan";
		for(int i=0;i<in.length();i++) {
			for(int j=0;j<in.length();j++) {
				if(i==j) {
					System.out.print(in.charAt(i));
				}
				else if(i+j=in.length()) {
					System.out.print(in.charAt(in.length()-i));
				}
				else {
					System.out.print(" ");
				}
			}
			System.out.println();
		}

	}

}
