package logic;

public class SumOfPowerOfDigits {

	public static void main(String[] args) {
		int num=123;
		String length=num+"";
		int size=length.length();
		int[] arr=new int[size];
		int i=0;
		while(num!=0) {
			int rem=num%10;
			arr[i]=rem;
			i+=1;
			num/=10;
			System.out.print("hello");
		}
		for(int j=arr[1];j<arr[size-1];j++) {
			System.out.println(j);
		}

	}

}
