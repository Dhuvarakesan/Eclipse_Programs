package logic;
import java.util.ArrayList;
public class MileStone4 {

	public static void main(String[] args) {
		int []a=new int[] {1,2,3,4,5,6,7};
		ArrayList<Integer>a1=new ArrayList<Integer>();
		for(int i:a) {
			boolean f=true;
			for(int j=2;j<i;j++) {
				if(i/j==0)
					f=false;
			}
			if(f)
				a1.add(i);
		}
		System.out.println(a1);
	}

}
