package logic;

public class Primebit {
	public int  primebet(int num) {
		boolean f= true;
		for(int i=2;i<=num/2;++i) {
				if(num%i==0) {
					f=false;
					break;
				}
			}
			if(f)
				System.out.print(num+" ");
			return num;
			

	}
	public static void main(String[] args) {
		int num=15;
		Primebit a=new Primebit();
	int result=	a.primebet(num);
	System.out.println(result);
	}

}
