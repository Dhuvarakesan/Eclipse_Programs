package logic;

public class FindDuplicate {

	public static void main(String[] args) {
		String in="1122334567";
		String output="";
		for(int i=0;i<in.length();i++) {
			for(int j=0;j<in.length();j++) {
				if(in.charAt(i)==in.charAt(j)) {
					output+=in.charAt(i);
					break;
				}
					
			}
		}

	}

}
