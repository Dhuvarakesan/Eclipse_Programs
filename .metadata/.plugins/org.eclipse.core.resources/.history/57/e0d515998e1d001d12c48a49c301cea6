package logic;

public class Zoho3 {

	public static void main(String[] args) {
		String in="(a+b)((a*B)";
		in=in.replaceAll("[^()]","");
		//System.out.println(in);
		//System.out.println(in.length());
		if(in.length()%2==0)
			System.out.println("VALID");
		else
			System.out.println("INVALID");

	}

}
