package logic;

public class StringNameProcess {

	public static void main(String[] args) {
	String input1="dhuvarake";
		int s1=input1.length();
		String s1f1="";
		String s1m1="";
		String s1e1="";
	        if(s1%3==0){
	            int temp=0;
	            for(int i=0;i<3;i++) {
	            s1f1+=Character.toString(input1.charAt(i));temp++;}
	            temp+=3;
			for (int i = temp; i <temp ;i++) {
				System.out.println(temp);
	                 s1m1+=Character.toString(input1.charAt(i));temp++;}
					for (int i = temp; i < 3/* +temp */;i++) {
	                 s1e1+=Character.toString(input1.charAt(i));temp++;
	                 if(temp==s1)
	                	 break;}
	        }
	        String res=s1f1+" "+s1m1+" "+s1e1+" ";
	        System.out.println(res);
	}

}
